{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "iife",
    "kolorist",
    "noopener",
    "noreferrer",
    "pnpm",
    "stroustrup",
    "stylelintrc",
    "trbl",
    "uncategorized",
    "unspaced",
    "vite",
    "vitepress",
    "vitest",
    "vuepress"
  ]
}
